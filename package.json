{
  "name": "Teka_Frontend",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "configs/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "prettier": "^2.8.8",
    "turbo": "^2.3.3",
    "typescript": "^5.1.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "eslint": "^9.23.0"
  }
}